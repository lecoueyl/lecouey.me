    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vue": "6.2.2",
    "node-sass": "4.14.1",
    "nuxt-purgecss": "1.0.0",
    "sass-loader": "8.0.2",
    "stylelint": "13.5.0",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.0.0",
    "stylelint-scss": "3.17.2"
