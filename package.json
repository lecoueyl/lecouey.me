{
  "name": "lecouey.me",
  "version": "1.0.0",
  "description": "Leonard Lecouey personal website",
  "author": "Leonard Lecouey <leonard.lecouey@gmail.com>",
  "private": true,
  "config": {
    "nuxt": {
      "host": "0.0.0.0"
    }
  },
  "scripts": {
    "clean": "rm -r node_modules .nuxt package-lock.json && npm install",
    "dev": "NODE_ENV=development nuxt",
    "build": "NODE_ENV=production nuxt build",
    "start": "NODE_ENV=production nuxt start",
    "generate": "NODE_ENV=production nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint '**/*.{vue,scss}'",
    "precommit": "npm run lint",
    "deploy": "npm run generate && bin/deploy.sh -s 'lecouey.me' -d '/opt/www/lecouey.me' -p '/dist'"
  },
  "dependencies": {
    "@nuxtjs/google-analytics": "^2.2.3",
    "@nuxtjs/sitemap": "^2.0.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "gsap": "^3.2.4",
    "iotacss": "^1.6.0",
    "nuxt": "^2.12.0",
    "nuxt-i18n": "^6.6.1",
    "nuxt-svg": "^0.1.6",
    "vue-check-view": "^0.3.0"
  },
  "devDependencies": {
    "@nuxtjs/stylelint-module": "^3.2.2",
    "ajv": "^6.12.0",
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.13.1",
    "nuxt-purgecss": "^0.2.1",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.2.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.15.0"
  }
}
