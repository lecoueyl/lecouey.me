    "nuxt-i18n": "6.11.1",
    "vue-check-view": "0.3.0"
  },
  "devDependencies": {
    "@nuxtjs/style-resources": "1.0.0",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/svg": "0.1.11",
    "babel-eslint": "10.1.0",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-import-resolver-webpack": "0.12.1",
    "eslint-loader": "4.0.2",
